{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/game-networking-lib-tests/game-networking-lib-tests.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/game-networking-lib-tests/game-networking-lib-tests.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/game-networking-lib-tests/game-networking-lib-tests.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "test",
            "command": "dotnet",
            "type": "process",
            "dependsOn": [
                "build"
            ],
            "group": {
                "kind": "test",
                "isDefault": true
            },
            "args": [
                "test",
                "${workspaceFolder}/game-networking-lib-tests/game-networking-lib-tests.csproj"
            ],
            "problemMatcher": "$tsc"
        }
    ]
}